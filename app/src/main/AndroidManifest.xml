<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_reccon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_reccon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppRecconProject"
        tools:targetApi="31">
        <activity
            android:name=".ActivityRecolectionDetail"
            android:exported="false"
            android:parentActivityName=".ActivityRecolection" />
        <activity
            android:name=".ActivitySettings"
            android:exported="false"
            android:parentActivityName=".ActivityMainModule" />
        <activity
            android:name=".ActivityMainModule"
            android:exported="false" />
        <activity
            android:name=".ActivityRecolection"
            android:exported="false"
            android:parentActivityName=".ActivityMainModule" />
        <activity
            android:name=".ActivityInformes"
            android:exported="false"
            android:parentActivityName=".ActivityMainModule"
            android:theme="@style/Theme.AppRecconProject.informes" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.AppRecconProject.splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>